{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "discountPrice": {
      "type": "integer"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "attributes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::attribute.attribute"
    },
    "options": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::attribute-term.attribute-term"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "products"
    }
  }
}
